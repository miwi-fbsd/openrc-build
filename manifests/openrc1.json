		"world-flags": {
			"default": [
				"MALLOC_PRODUCTION=YES",
				"WITHOUT_ASSERT_DEBUG=ON"
			]
		},
		"kernel-flags": {
			"default": [
				"KERNCONF=GENERIC-NODEBUG"
			]
		},
		"branch":"trueos/master",
		"repo":"https://github.com/trueos/trueos"
	},
	"iso": {
		"auto-install-packages": {
			"default": [
				"security/ca_root_nss",
				"security/sudo",
				"sysutils/ipmitool",
				"sysutils/dmidecode",
				"sysutils/tmux",
				"security/gnupg",
				"sysutils/sysup",
				"textproc/jq"
			]
		},
		"auto-install-script": "",
		"dist-packages": {
			"default": [
				"sysutils/openzfs"
			]
		},
		"dist-packages-glob": {
			"default": [
				"os-minimal-kernel",
				"os-minimal-userland",
				"os-nozfs-kernel",
				"os-nozfs-userland"
			]
		},
		"file-name": "TrueOS-Snapshot-Builder-x64-%%GITHASH%%-%%DATE%%",
		"install-script": "",
		"iso-base-packages": {
			"default": [
				"os-generic-userland",
				"os-generic-kernel"
			]
		},
		"iso-packages": {
			"default": [
				"sysutils/openzfs",
				"sysutils/pc-installdialog",
				"sysutils/pc-sysinstall",
				"textproc/jq"
			]
		},
		"os-flavors": {
			"generic": {
				"description":"Default TrueOS world / kernel"
			},
			"minimal":{
				"description":"Minimal world with less optional features."
			},
			"nozfs":{
				"description":"Base system without built-in ZFS"
			},
			"openzfs":{
				"description":"Base system with OpenZFS from ports",
				"flavor":"nozfs",
				"packages":"openzfs"
			}
		},
		"overlay": {
		},
		"prune": {
			"default": [
				"METALOG",
				"/usr/local/share/examples",
				"/usr/local/include",
				"/usr/bin/cc*",
				"/usr/bin/clang*",
				"/usr/bin/cpp",
				"/usr/bin/cpp",
				"/usr/bin/c++",
				"/usr/bin/lldb",
				"/usr/bin/ld.lld",
				"/usr/bin/llvm*",
				"/usr/bin/objdump*",
				"/usr/bin/svn*",
				"/usr/lib/clang",
				"/usr/share/man",
				"/usr/share/i18n",
				"/usr/share/openssl/man",
				"/usr/include",
				"/usr/lib32"
			]
		}
